{
  "compilerOptions": {
    "outDir": "./lib",
    "incremental": true,
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true,
    "declaration": true,
    "strict": true,
    "skipLibCheck": true
  }
}
